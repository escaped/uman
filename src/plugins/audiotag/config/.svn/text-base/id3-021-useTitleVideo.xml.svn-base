<task>
    <general group="10" type="id3" >
        <icon resource=":/types/font.png" />
        <description><![CDATA[Change #TITLE via ID3 tag from audio file. Append "[VIDEO]" if video present.]]></description>
        <tooltip/>
    </general>
    <id3 target="title" schema="%1%2" >
        <data ignoreEmpty="target" source="title" />
        <data if="hasVideo" text=" [VIDEO]" />
    </id3>
</task>
